{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "types": [
      "@types/node"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}
